ALTER PROCEDURE [dbo].[sp_DeleteTasksByPrimaryKey]
	@TASK_ID int,
	@EMP_ID int
AS

DELETE FROM [dbo].[TASKS]
 WHERE 
	[TASK_ID] = @TASK_ID AND 
	[EMP_ID] = @EMP_ID


