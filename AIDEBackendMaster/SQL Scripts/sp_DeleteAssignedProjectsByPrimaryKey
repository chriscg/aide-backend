CREATE PROCEDURE [dbo].[sp_DeleteAssignedProjectsByPrimaryKey]
	@EMP_ID int,
	@PROJ_ID int
AS

DELETE FROM [dbo].[ASSIGNED_PROJECTS]
 WHERE 
	[EMP_ID] = @EMP_ID AND 
	[PROJ_ID] = @PROJ_ID


