CREATE PROCEDURE [dbo].[sp_PROJECT_Insert]
	@PROJ_ID int ,
	@PROJ_NAME varchar(20) ,
	@CATEGORY smallint ,
	@BILLABILITY smallint,
	@EMP_ID int

AS

INSERT [dbo].[PROJECT]
(
	[PROJ_ID],
	[PROJ_NAME],
	[CATEGORY],
	[BILLABILITY],
	[EMP_ID]

)
VALUES
(
	@PROJ_ID,
	@PROJ_NAME,
	@CATEGORY,
	@BILLABILITY,
	@EMP_ID

)



