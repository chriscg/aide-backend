USE [AIDE]
GO
/****** Object:  StoredProcedure [dbo].[sp_UpdateSabaXref]    Script Date: 5/31/2019 4:48:49 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE PROCEDURE [dbo].[sp_UpdateSabaXref] 
@SABA_ID INT,
@EMP_ID INT,  
@DATE_COMPLETED DATE
AS

BEGIN

UPDATE SABA_XREF SET 
DATE_COMPLETED = @DATE_COMPLETED 
WHERE SABA_ID = @SABA_ID AND EMP_ID = @EMP_ID

END
