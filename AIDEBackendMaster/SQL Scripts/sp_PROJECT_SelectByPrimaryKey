/****** Object:  StoredProcedure [dbo].[sp_PROJECT_SelectByPrimaryKey]    Script Date: 5/31/2019 4:18:04 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE PROCEDURE [dbo].[sp_PROJECT_SelectByPrimaryKey]
	@PROJ_ID int
AS

	SELECT 
		[PROJ_ID], [PROJ_NAME], [CATEGORY], [BILLABILITY]
	FROM [dbo].[PROJECT]
	WHERE 
			[PROJ_ID] = @PROJ_ID


